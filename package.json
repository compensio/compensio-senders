    "dist"
  ],
  "homepage": "https://github.com/compens-io/senders#readme",
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "jest": "^24.8.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
